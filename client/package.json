{
  "name": "new",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "init": "node ./utils/init.js",
    "dev": "vue-cli-service serve --open",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "colors": "^1.2.3",
    "request": "^2.85.0",
    "vue": "^2.5.16"
  },
  "devDependencies": {
    "prompts": "^0.1.8",
    "@vue/cli-plugin-babel": "^3.0.0-beta.7",
    "@vue/cli-plugin-eslint": "^3.0.0-beta.7",
    "@vue/cli-service": "^3.0.0-beta.7",
    "vue-template-compiler": "^2.5.13"
  },
  "babel": {
    "presets": [
      "@vue/app"
    ]
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "vue/no-parsing-error": [
        2,
        {
          "x-invalid-end-tag": false
        }
      ]
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
